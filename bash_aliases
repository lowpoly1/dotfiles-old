alias ls="ls -F"
alias ll="ls -lF"
alias la="ls -aF"
alias lla="ls -laF"
alias llam="ls -laF | more"
alias llama="ls -laF * | more"
alias llamr="ls -laFR | more"
alias lm="ls -F | more"
alias ..="cd .."
alias ...="cd ../.."
alias .3="cd ../../.."
alias .4="cd ../../../.."
alias .5="cd ../../../../.."
alias cls="clear"
